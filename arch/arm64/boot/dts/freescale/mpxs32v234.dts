/dts-v1/;
#include "mpxs32v234.dtsi"

&fec {
	phy-mode = "rgmii";
	phy-handle = <&phy0>;
	status = "okay";
	mdio {
		#address-cells = <1>;
		#size-cells = <0>;
		phy0: ethernet-phy@1 {
			reg = <1>;
		};
	};
};

&i2c2 {
    rtc@51 {
        compatible = "nxp,pcf85263";
	reg = <0x51>;
        quartz-load-capacitance = <2>;
        quartz-drive-strength = <1>;
    };
};
